    "rollup": "^2.10.9",
    "rollup-plugin-copy": "^3.3.0",
    "rollup-plugin-delete": "^1.2.0",
    "rollup-plugin-html2": "^1.1.0",
    "rollup-plugin-node-externals": "^2.2.0",
    "rollup-plugin-progress": "^1.1.2",
    "rollup-plugin-serve": "^1.0.1"
