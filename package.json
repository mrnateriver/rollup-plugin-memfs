    "url": "https://github.com/mrnateriver/rollup-plugin-memory-fs/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/mrnateriver/rollup-plugin-memory-fs"
  },
  "files": [
    "dist"
  ],
  "devDependencies": {
    "@rollup/plugin-commonjs": "^12.0.0",
    "@rollup/plugin-node-resolve": "^8.0.0",
    "axios": "^0.19.2",
    "cross-env": "^7.0.2",
    "eslint": "^7.1.0",
